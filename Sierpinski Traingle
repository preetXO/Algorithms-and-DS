        "import matplotlib.pyplot as plt\n",
        "from random import choice\n",
        "%matplotlib inline"
      ],
      "execution_count": 49,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "E5giSceaV_aR"
      },
      "source": [
        "def sierpinski_traingle(n):\n",
        "  '''\n",
        "  --- Thie funtion will take in a value and print serpinski triangle ---\n",
        "  --- Parameters ---\n",
        "  n = 'the values of x\n",
        "  '''\n",
        "  X = [0]\n",
        "  Y = [0]\n",
        "  for i in range(n-1):\n",
        "    formula = choice([1,2,3])\n",
        "    if formula == 1:\n",
        "      X.append(0.5 * X[i])\n",
        "      Y.append(0.5 * Y[i])\n",
        "    elif formula == 2:\n",
        "      X.append((0.5 * X[i]) + 0.5)\n",
        "      Y.append((0.5 * Y[i]) + 0.5)\n",
        "    else:\n",
        "      X.append((0.5 * X[i]) + 1)\n",
        "      Y.append((0.5 * Y[i]))\n",
        "  plt.plot(X,Y, 'o', color = 'g')\n",
        "  plt.xlabel(\"Horizontal axis\")\n",
        "  plt.ylabel(\"Vertical axis\")\n",
        "  plt.title(f\"Seirpinksi triangle of {n} values\")\n",
        "  plt.rc('figure', figsize = (10,8))\n",
        "  plt.show()"
      ],
      "execution_count": 72,
      "outputs": []
    },
    {
      "cell_type": "code",
